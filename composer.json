{
    "name": "php-pm/httpkernel-adapter",
    "description": "PHP-PM adapter for Symfony HTTPKernel.",
    "license": "MIT",
    "require": {
        "php-pm/php-pm": "^2.0",
        "symfony/http-foundation": "^3.4|^4.2.12|^5.0",
        "symfony/http-kernel": "^3.4|^4.0|^5.0",
        "guzzlehttp/psr7": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/framework-bundle": "^3.4|^4.1.12|^5.0",
        "symfony/yaml": "^3.4|^4.0|^5.0",
        "doctrine/annotations": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "PHPPM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPPM\\Tests\\": "tests"
        }
    }
}
