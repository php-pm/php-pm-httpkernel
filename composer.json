{
    "name": "php-pm/httpkernel-adapter",
    "description": "PHP-PM adapter for Symfony HTTPKernel.",
    "license": "MIT",
    "require": {
        "php-pm/php-pm": "^2.0",
        "symfony/http-foundation": "^4.1.13|^5.1.5|^6.0",
        "symfony/http-kernel": "^4.0|^5.0|^6.0",
        "guzzlehttp/psr7": "^1.5|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/framework-bundle": "^4.1.13|^5.1.5|^6.0",
        "symfony/yaml": "^4.0|^5.0|^6.0",
        "doctrine/annotations": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "PHPPM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPPM\\Tests\\": "tests"
        }
    }
}
